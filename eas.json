{
  "cli": {
    "version": ">= 14.0.3",
    "appVersionSource": "remote"
  },
  "build": {
    "development": {
      "developmentClient": true,
      "distribution": "internal"
    },
    "preview": {
      "distribution": "internal"
    },
    "production": {
      "autoIncrement": true,
      "env": {
        "SUPABASE_DB_URL": "SUPABASE_DB_URL",
        "SUPABASE_DB_KEY": "SUPABASE_DB_KEY"
      }
    }
  },
  "submit": {
    "production": {}
  }
}
